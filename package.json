{
  "name": "remi-expose",
  "version": "0.2.0",
  "description": "A remi extension that adds the expose method to the target",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/{**/,/}*.js",
    "coverage": "istanbul cover _mocha test/{**/,/}*.js -- -R spec",
    "precoveralls": "istanbul cover _mocha test/{**/,/}*.js --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remijs/remi-expose.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "remi",
    "extension",
    "expose"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remijs/remi-expose/issues"
  },
  "homepage": "https://github.com/remijs/remi-expose#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "plugiator": "^0.1.1",
    "remi": "^0.5.0"
  },
  "dependencies": {
    "camelcase": "^2.1.0"
  }
}
